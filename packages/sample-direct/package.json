{
  "name": "sample-direct",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for sample-direct goes here",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js app config lib server tests",
    "start": "ember serve",
    "test": "ember test --test-port=0",
    "test:custom-bundles": "CUSTOMIZE_BUNDLES=/js/vendor.js ember test --test-port=0",
    "test:custom-bundles-top": "CUSTOMIZE_BUNDLES=/top-level-vendor.js ember test --test-port=0",
    "test:custom-csp": "CUSTOMIZE_CSP=true ember test --test-port=0",
    "test:fastboot": "qunit fastboot-tests/dev-fastboot-test.js",
    "test:custom-bundles-fastboot": "CUSTOMIZE_BUNDLES=/js/custom-vendor.js qunit fastboot-tests/dev-fastboot-test.js",
    "test:prod": "qunit fastboot-tests/prod-fastboot-test.js"
  },
  "devDependencies": {
    "@ef4/scoped-lib": "*",
    "a-dependency": "*",
    "a-module-dependency": "*",
    "broccoli-asset-rev": "*",
    "ember-ajax": "*",
    "ember-auto-import": "*",
    "ember-cli": "*",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-dependency-checker": "*",
    "ember-cli-fastboot": "*",
    "ember-cli-htmlbars": "*",
    "ember-cli-htmlbars-inline-precompile": "*",
    "ember-cli-inject-live-reload": "*",
    "ember-cli-qunit": "*",
    "ember-cli-shims": "*",
    "ember-cli-sri": "*",
    "ember-cli-uglify": "*",
    "ember-export-application-global": "*",
    "ember-load-initializers": "*",
    "ember-maybe-import-regenerator": "*",
    "ember-resolver": "*",
    "ember-source": "*",
    "fastboot": "*",
    "inner-lib": "*",
    "inner-lib2": "*",
    "jsdom": "*",
    "loader.js": "*",
    "lodash-es": "*",
    "moment": "*",
    "sample-v2-addon":"*",
    "qunit": "*"
  },
  "engines": {
    "node": ">= 8.*"
  },
  "ember-addon": {
    "paths": [
      "lib/bundle-switcher",
      "../csp-switcher"
    ]
  }
}
