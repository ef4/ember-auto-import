{
  "name": "sample-direct",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for sample-direct goes here",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:custom-bundles": "CUSTOMIZE_BUNDLES=/js/vendor.js ember test --test-port=0",
    "test:custom-bundles-top": "CUSTOMIZE_BUNDLES=/top-level-vendor.js ember test --test-port=0",
    "test:custom-csp": "CUSTOMIZE_CSP=true ember test --test-port=0",
    "test:fastboot": "qunit fastboot-tests/dev-fastboot-test.js",
    "test:custom-bundles-fastboot": "CUSTOMIZE_BUNDLES=/js/custom-vendor.js qunit fastboot-tests/dev-fastboot-test.js",
    "test:prod": "qunit fastboot-tests/prod-fastboot-test.js"
  },
  "devDependencies": {
    "@ef4/scoped-lib": "*",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "*",
    "@glimmer/component": "*",
    "@glimmer/tracking": "*",
    "a-dependency": "*",
    "a-module-dependency": "*",
    "broccoli-asset-rev": "*",
    "ember-auto-import": "*",
    "ember-cli": "*",
    "ember-cli-babel": "*",
    "ember-cli-dependency-checker": "*",
    "ember-cli-fastboot": "*",
    "ember-cli-htmlbars": "*",
    "ember-cli-inject-live-reload": "*",
    "ember-cli-sri": "*",
    "ember-cli-terser": "*",
    "ember-export-application-global": "*",
    "ember-load-initializers": "*",
    "ember-maybe-import-regenerator": "*",
    "ember-page-title": "*",
    "ember-qunit": "*",
    "ember-resolver": "*",
    "ember-source": "*",
    "ember-template-lint": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-prettier": "*",
    "fastboot": "*",
    "inner-lib": "*",
    "inner-lib2": "*",
    "jsdom": "*",
    "loader.js": "*",
    "lodash-es": "*",
    "moment": "*",
    "npm-run-all": "*",
    "prettier": "*",
    "qunit": "*",
    "qunit-dom": "*"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/bundle-switcher",
      "../csp-switcher"
    ]
  }
}
